{
    "name": "figoli-quinn/siteup",
    "description": "A package to determine if a website url is up.",
    "authors": [
        {
            "name": "Steven Quinn",
            "email": "stevenquinn@me.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^5.8 || ^6.0 || ^.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
	"autoload": {
		"psr-4": {
			"FigoliQuinn\\SiteUp\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"FigoliQuinn\\SiteUp\\SiteUpServiceProvider"
			],
			"aliases": {
            	"SiteUpChecker": "FigoliQuinn\\SiteUp\\Facades\\SiteUpChecker"
        	}
		}
	}
}
